{
  "id": "example-project",
  "title": "Example Project",
  "description": "A template for creating new projects",
  "date": "2024-12",
  "tags": ["p5js", "template"],
  "libraries": ["p5"],
  "entryFile": "/projects/example-project/index.ts",
  "controls": [
    {
      "type": "slider",
      "label": "Speed",
      "key": "speed",
      "default": 1,
      "min": 0.1,
      "max": 5,
      "step": 0.1
    },
    {
      "type": "toggle",
      "label": "Show Grid",
      "key": "showGrid",
      "default": true
    },
    {
      "type": "color",
      "label": "Background Color",
      "key": "bgColor",
      "default": "#000000"
    },
    {
      "type": "select",
      "label": "Mode",
      "key": "mode",
      "default": "organic",
      "options": [
        { "label": "Organic", "value": "organic" },
        { "label": "Geometric", "value": "geometric" },
        { "label": "Abstract", "value": "abstract" }
      ]
    }
  ],
  "github": "https://github.com/user/example-project"
}
