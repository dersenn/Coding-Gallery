[
  {
    "id": "noise-field",
    "title": "Perlin Noise Field",
    "description": "Flow field visualization using Perlin noise",
    "date": "2024-03",
    "tags": ["p5js", "generative", "noise"],
    "libraries": ["p5", "simplex-noise"],
    "entryFile": "/projects/noise-field/index.ts",
    "controls": [
      {
        "type": "slider",
        "label": "Particle Count",
        "key": "particleCount",
        "default": 1000,
        "min": 100,
        "max": 5000,
        "step": 100
      },
      {
        "type": "slider",
        "label": "Flow Speed",
        "key": "flowSpeed",
        "default": 0.5,
        "min": 0.1,
        "max": 2,
        "step": 0.1
      },
      {
        "type": "toggle",
        "label": "Show Trails",
        "key": "showTrails",
        "default": true
      },
      {
        "type": "color",
        "label": "Background Color",
        "key": "bgColor",
        "default": "#000000"
      }
    ]
  }
]
